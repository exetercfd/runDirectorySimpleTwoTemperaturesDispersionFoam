set terminal png
set output "plot_residuals.png"
set logscale y
set format y "1e%T"
set xlabel "Iterations (-)"
set ylabel "Residuals (-)"
set key right top box 1 box opaque
#set xtics 0,100,600
set yrange [10e-18:10e1]
#set yrange [296:306]
plot "postProcessing/residuals/0/residuals.dat" using 1:2 with lines lc rgb "red" title "U residuals" ,\
	 "postProcessing/residuals/0/residuals.dat" using 1:3 with lines lc rgb "black" title "P residuals" ,\
	 "postProcessing/residuals/0/residuals.dat" using 1:4 with lines lc rgb "blue" title "Tf residuals" ,\
	 "postProcessing/residuals/0/residuals.dat" using 1:5 with lines lc rgb "green" title "Ts residuals"

